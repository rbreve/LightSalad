Activity
<table>
  <tr>
    <th>Datetime</th>
    <th>User</th>
    <th>Name</th>
    <th>List</th>
    <th>Text</th>
    <th>Comment</th>
    <th>Feature</th>
    <th>Action</th>
    <th>Content</th>
  </tr>
  <% for log in @logs %>
    <tr>
      <td><%=h log.datetime %></td>
      <td><%=h log.user_id %></td>
      <td><%=h log.name %></td>
      <td><%=h log.list_id %></td>
      <td><%=h log.text %></td>
      <td><%=h log.comment_id %></td>
      <td><%=h log.feature_id %></td>
      <td><%=h log.action %></td>
      <td><%=h log.content %></td>
      <td><%= link_to "Show", log %></td>
      <td><%= link_to "Edit", edit_log_path(log) %></td>
      <td><%= link_to "Destroy", log, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Log", new_log_path %></p>
